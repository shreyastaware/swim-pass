# Google Sheets Configuration
# REQUIRED: Get your API key from: https://console.cloud.google.com/apis/credentials
EXPO_PUBLIC_GOOGLE_SHEETS_API_KEY=

# REQUIRED: Get your Spreadsheet ID from the URL: https://docs.google.com/spreadsheets/d/{SPREADSHEET_ID}/edit
EXPO_PUBLIC_GOOGLE_SPREADSHEET_ID=

# Uploadcare Configuration (for image uploads)
# REQUIRED: Get your key from: https://uploadcare.com/
EXPO_PUBLIC_UPLOADCARE_PUBLIC_KEY=

# Other environment variables (Optional - for Create platform)
EXPO_PUBLIC_PROJECT_GROUP_ID=
EXPO_PUBLIC_BASE_URL=
EXPO_PUBLIC_PROXY_BASE_URL=
EXPO_PUBLIC_HOST=
EXPO_PUBLIC_BASE_CREATE_USER_CONTENT_URL=
EXPO_PUBLIC_LOGS_ENDPOINT=
EXPO_PUBLIC_CREATE_TEMP_API_KEY=
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=
